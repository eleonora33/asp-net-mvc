@model List<OrderDetailsViewModel>

<h2>There are @ViewData["NumberOfOrders"] orders made in our app.</h2>
<h4>@ViewData["Title"]</h4>
    <ol>
    @for(int i =0; i< Model.Count; i++)
    {
        
    var tmp = @Model.ElementAt(i);
      <p>=====================================================================================</p>
        <partial name="~/Views/Shared/Partials/_OrderDetailsPartial.cshtml" for = "@tmp"/>
    }

</ol>